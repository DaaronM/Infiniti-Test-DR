@inherits System.Web.Mvc.WebViewPage<Intelledox.ViewModel.Core.InkQuestionViewModel>
@{
    Layout = "~/Views/Shared/_WizardSubpage.cshtml";
}

@section AddToHead
{
    <script src="~/scripts/jquery-ui-1.12.1.custom.min.js" type="text/javascript"></script>
    <script src="~/scripts/wizard.js?v=10.2.11" type="text/javascript"></script>
    @if (Infiniti.MvcControllers.ApplicationSettings.UseCdn)
    {
        <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js" integrity="sha384-Tc5IQib027qvyjSMfHjOMaLkfuWVxZxUPnCJA7l2mCWNIpG9mGCD8wGNIcPD7Txa" crossorigin="anonymous"></script>
    }
    else
    {
        <script src="~/scripts/bootstrap.min.js?v=337"></script>
    }

}
<form method="post">
    <div>
        <div id="contentinner" class="base1" style="display: none">
            <div class="body">
                @Html.Partial("Ink", Model)
            </div>
        </div>
    </div>
</form>
